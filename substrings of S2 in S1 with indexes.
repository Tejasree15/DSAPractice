public class Main
{
	public static void main(String[] args) {
		System.out.println("Hello World");
		String s1 = "hellokitty123kello123";
		String s2 = "123";
		boolean flag = false;

		int l1 = s1.length();
		int l2 = s2.length();
		for(int i = 0; i <= l1-l2; i++) {
			String sub = s1.substring(i,i+l2);
			if(sub.equals(s2)) {
				System.out.println(s2 + " found at index "+ i);
				flag = true;
			}
		}
		
		if(!flag){
		    System.out.println("Not Found");
		}

	}
}
